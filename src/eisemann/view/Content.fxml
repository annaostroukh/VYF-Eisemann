<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane prefHeight="700.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eisemann.view.ContentController">
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Button fx:id="uploadNoFlash" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="30.0" prefWidth="350.0" text="Upload photo without flash" GridPane.columnIndex="2" GridPane.rowIndex="4">
         <GridPane.margin>
            <Insets bottom="10.0" left="25.0" right="10.0" top="10.0" />
         </GridPane.margin>
      </Button>
      <Button fx:id="getResult" mnemonicParsing="false" onAction="#handleResultAction" prefHeight="30.0" prefWidth="350.0" text="Get result" GridPane.columnIndex="3" GridPane.rowIndex="4">
         <GridPane.margin>
            <Insets bottom="10.0" left="25.0" right="10.0" top="10.0" />
         </GridPane.margin>
      </Button>
      <ImageView fx:id="imageViewNoFlash" fitHeight="400.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2">
         <GridPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </GridPane.margin></ImageView>
      <ImageView fx:id="imageViewResult" fitHeight="400.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="3">
         <GridPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </GridPane.margin></ImageView>
      <ImageView fx:id="imageViewFlash" fitHeight="400.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true" GridPane.columnIndex="2" GridPane.rowIndex="6">
         <GridPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </GridPane.margin></ImageView>
      <Button fx:id="uploadFlash" mnemonicParsing="false" onAction="#handleButtonAction" prefHeight="30.0" prefWidth="350.0" text="Upload photo with flash" GridPane.columnIndex="2" GridPane.rowIndex="7">
         <GridPane.margin>
            <Insets bottom="10.0" left="25.0" right="10.0" top="10.0" />
         </GridPane.margin>
      </Button>
      <HBox prefHeight="30.0" prefWidth="250.0" GridPane.columnIndex="3" GridPane.rowIndex="6">
         <children>
            <ImageView fx:id="imageViewResultTest" fitHeight="400.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </ImageView>
         </children>
      </HBox>
   </children>
</GridPane>
